{
  "name": "baum/baum",
  "type": "library",
  "description": "Baum is an implementation of the Nested Set pattern for Eloquent models.",
  "keywords": ["nested set", "laravel", "laravel 4", "laravel 5", "eloquent", "database"],
  "license": "MIT",
  "authors": [
    {
      "name": "Estanislau Trepat",
      "email": "estanis@etrepat.com",
      "homepage": "http://etrepat.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/database": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~5.9.0",
    "illuminate/events": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~5.9.0",
    "illuminate/support": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~5.9.0"
  },
  "require-dev": {
    "doctrine/dbal": "~2.9.2",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "~6.5.14|^7.0",
    "psy/psysh": "@stable",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Baum\\": "src/"
    }
  },
  "autoload-dev": {
      "files": [
        "tests/suite/Support/helpers.php"
      ],
      "psr-4": {
          "Baum\\Tests\\": "tests/suite/"
      }
  },
  "extra": {
    "laravel": {
        "providers": [
          "Baum\\BaumServiceProvider"
        ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
